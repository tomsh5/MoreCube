ityType, entityId) {\n    return query(entityType)\n        .then(entities => {\n            const idx = entities.findIndex(entity => entity._id === entityId);\n         \n            entities.splice(idx, 1)\n            _save(entityType, entities)\n        })\n}\n\nfunction _save(entityType, entities) {\n    localStorage.setItem(entityType, JSON.stringify(entities))\n}\n\n\nfunction clearStorage(entity){\n    localStorage.removeItem(entity);\n\n}\n\n// function _makeId(length = 5) {\n//     var text = \"\";\n//     var possible = \"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789\";\n//     for (var i = 0; i < length; i++) {\n//         text += possible.charAt(Math.floor(Math.random() * possible.length));\n//     }\n//     return text;\n// }","C:\\Users\\tomsh\\Desktop\\Projects\\the-cube\\src\\cmps\\Modal\\Modal.jsx",[],"C:\\Users\\tomsh\\Desktop\\Projects\\the-cube\\src\\hooks\\useModal.js",[],["77","78"],"C:\\Users\\tomsh\\Desktop\\Projects\\the-cube\\src\\cmps\\Footer\\Footer.jsx",["79","80"],"import React, { useState, useEffect } from 'react';\r\nimport './Footer.scss';\r\n\r\n\r\nexport function Footer() {\r\n\r\n    return (\r\n        <footer className=\"flex justify-center\">\r\n\r\n\r\n           <label className=\"right-res\">Â© All rights reserved 2021</label> \r\n           <span>UX/UI: <a href=\"https://www.linkedin.com/in/mor-gavni-496234198\">Mor Gavni</a></span>\r\n           <span>Developed By: <a href=\"https://www.linkedin.com/in/tom-shalem\">Tom shalem</a> </span>\r\n\r\n\r\n        </footer>\r\n    )\r\n\r\n}","C:\\Users\\tomsh\\Desktop\\Projects\\the-cube\\src\\i18n.js",[],"C:\\Users\\tomsh\\Desktop\\Projects\\the-cube\\src\\cmps\\SubShare\\SubShare.jsx",["81"],"import React, { useState, useEffect } from 'react';\r\nimport './SubShare.scss'\r\nimport {\r\n    FacebookShareButton,\r\n    EmailShareButton,\r\n    TwitterShareButton,\r\n    LinkedinShareButton,\r\n    WhatsappShareButton,\r\n    FacebookIcon,\r\n    EmailIcon,\r\n 